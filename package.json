{
  "name": "smartpin-tpo",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "npm run generate-types:local-safe && next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:strict": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --max-warnings=0",
    "test": "jest --watch",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "generate-types": "npx supabase gen types typescript --project-id vhtbinssqbzcjmbgkseo > src/lib/database.types.ts",
    "generate-types:local": "npx supabase gen types typescript --local > src/lib/database.types.ts || echo 'Warning: Local Supabase not running, skipping type generation'",
    "generate-types:local-safe": "npx supabase gen types typescript --local > src/lib/database.types.ts 2>/dev/null || echo 'Using existing types - local Supabase not available'",
    "generate-types:safe": "node scripts/generate-types-safe.js",
    "types:watch": "npx nodemon --watch supabase/migrations --ext sql --exec \"npm run generate-types\"",
    "db:reset": "npx supabase db reset && npm run generate-types:local",
    "types:gen": "SUPABASE_PROJECT_ID=vhtbinssqbzcjmbgkseo DB_TYPES_PATH=src/lib/database.types.ts bash ./scripts/types-gen.sh",
    "ssot:check": "node ./scripts/check-enums.mjs",
    "build:strict": "npm run types:gen && npm run ssot:check && next build"
  },
